<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tanush.scminventory">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.tanush.scminventory.loginActivities.OverViewLoginActivity"></activity>
        <activity android:name="com.tanush.scminventory.loginActivities.AdminLoginActivity" />
        <activity android:name="com.tanush.scminventory.ProductOverviewActivity" />
        <activity android:name="com.tanush.scminventory.StockEditActivity" />
        <activity
            android:name="com.tanush.scminventory.AddProductActivity"
            android:parentActivityName="com.tanush.scminventory.AdminActivity" />
        <activity android:name="com.tanush.scminventory.AdminActivity" />
        <activity android:name="com.tanush.scminventory.OverViewActivity" />
        <activity
            android:name="com.tanush.scminventory.MainActivity"
            android:launchMode="singleTop"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>